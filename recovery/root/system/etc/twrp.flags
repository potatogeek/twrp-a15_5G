/spmfw		   emmc	       /dev/block/platform/bootdevice/by-name/spmfw	         flags=display="SPMFW";backup=1
/boot_para	   emmc	       /dev/block/platform/bootdevice/by-name/boot_para	         flags=display="BOOT_PARA";backup=1;flashimg=1
